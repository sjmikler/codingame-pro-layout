{
  "manifest_version": 3,
  "name": "CodinGame Pro Layout",
  "version": "2.2",
  "description": "Overhauls the CodinGame IDE with a cleaner layout and adds automatic code synchronization.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.codingame.com/*"
      ],
      "css": [
        "src/styles.css"
      ],
      "js": [
        "src/content.js"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "{0b1eb558-4aa2-4509-91ef-9aab8cabe4ed}"
    }
  }
}
