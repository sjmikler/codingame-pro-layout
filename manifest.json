{
  "manifest_version": 3,
  "name": "CodinGame Pro Layout",
  "version": "1.4",
  "description": "Remove the redundant Code Editor and replace it with a larger Console view.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.codingame.com/*"
      ],
      "css": [
        "src/styles.css"
      ],
      "js": [
        "src/content.js"
      ]
    }
  ]
}
